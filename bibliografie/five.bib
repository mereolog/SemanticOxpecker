@article{Oury2009293,
title = "Interrelationships between meat quality traits, texture measurements and physicochemical characteristics of M. rectus abdominis from Charolais heifers",
journal = "Meat Science",
volume = "83",
number = "2",
pages = "293 - 301",
year = "2009",
note = "",
issn = "0309-1740",
